            <% products.forEach((product)=>{ %>
                <% if( product.status == "Available"){ %>
                    <div class="product">
                  <% } else{ %>  
                    <div class="product opacity">
                 <% } %>
                
                    <div class="topBox">
                        <a href="product/<%= product._id %>"><img src="/uploads/<%= product.productImages[0].productImage1 %>" alt=""></a>
                    </div>
                    <div class="bottomBox">
                        <div class="left">
                            <p class="owner"><%= product.ownerName %></p>
                            <p class="productName"><%= product.productName %></p>
                            <p class="stars">⭐⭐⭐⭐⭐</p>
                            <p class="price"><span><%= product.productPrice %></span> per day.</p>
                            <% if(product.status == "Available"){ %>
                                
                              <% } else{ %>  
                                <p class="unavailable">***Currently Available***</p>
                             <% } %>
                            
                        </div>
                        <div class="right">
                            <div class="cartButton">
                                <a href="product/<%= product._id %>"><i class="fa-solid fa-cart-plus"></i></a>
                            </div>
                        </div>
                    </div>
                </div>

                
            <% }) %>
        
